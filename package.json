{
	"name": "x-processor-utils",
	"displayName": "xProcessor",
	"description": "A document content processor extension for VS Code.",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.41.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.xReverse",
		"onCommand:extension.xToLowerCase",
		"onCommand:extension.xToUpperCase"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.xReverse",
				"title": "xReverse",
				"category": "xProcessor"
			},
			{
				"command": "extension.xToLowerCase",
				"title": "xToLowerCase",
				"category": "xProcessor"
			},
			{
				"command": "extension.xToUpperCase",
				"title": "xToUpperCase",
				"category": "xProcessor"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.41.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"typescript": "^3.6.4",
		"tslint": "^5.20.0",
		"vscode-test": "^1.2.2"
	}
}
